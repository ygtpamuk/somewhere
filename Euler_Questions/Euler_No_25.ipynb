    "%%time\n",
    "# Solution 1\n",
    "\n",
    "fib_l=[1,0]\n",
    "t=0\n",
    "while True:\n",
    "    t+=1 \n",
    "    z=fib_l[-1]\n",
    "    y=fib_l[-2]\n",
    "    fib_l.append(z+y)\n",
    "    fib_l.pop(0)\n",
    "    int_count=len(str(fib_l[-1]))\n",
    "    if int_count==1000:\n",
    "        print(t)\n",
    "        break \n",
    "    \n",
